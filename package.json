    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.44.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-vue": "^9.2.0",
    "jsdom": "^22.1.0",
    "npm-run-all2": "^6.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.5.1",
    "typescript": "~5.2.0",
    "vite": "^4.4.9",
    "vitest": "0.34.6",
    "vue-tsc": "^1.8.11"
